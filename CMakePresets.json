    "configurePresets": 
    [
        {
            "name": "base",
            "hidden": true,
            "displayName": "Base configuration (hidden)",
            "toolchainFile": "$env{VCPKG_ROOT}/scripts/buildsystems/vcpkg.cmake",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++",
                "CMAKE_WINDOWS_EXPORT_ALL_SYMBOLS": "ON",
                "PRESET_NAME": "${presetName}"
            },
            "environment": {
                "BUILD_FOLDER": "cmake-build"
            }
        },



        {
            "name": "Release",
            "inherits": [
                "base"
            ],
            "hidden": true,
            "displayName": "Release build type (hidden)",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        },
        {
            "name": "Debug",
            "inherits": [
                "base"
            ],
            "hidden": true,
            "displayName": "Debug build type (hidden)",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_DEBUG_POSTFIX": "_d",
                "CHECK_MEMORY_LEAKS": true
            }
        },

        
        
        {
            "name": "Linux",
            "inherits": [
                "base"
            ],
            "hidden": true,
            "displayName": "Linux (hidden)",
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Linux"
            },
            "cacheVariables": {
                "CMAKE_EXECUTABLE_SUFFIX_CXX": ".out",
                "CMAKE_SHARED_LIBRARY_SUFFIX_CXX": ".so",
                "VCPKG_TARGET_TRIPLET": "x64-linux",
                "VCPKG_HOST_TRIPLET": "x64-linux"
            }
        },
        {
            "name": "Windows",
            "inherits": [
                "base"
            ],
            "hidden": true,
            "displayName": "Windows (hidden)",
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Windows"
            },
            "cacheVariables": {
                "CMAKE_EXECUTABLE_SUFFIX_CXX": ".exe",
                "CMAKE_SHARED_LIBRARY_SUFFIX_CXX": ".dll",
                "VCPKG_TARGET_TRIPLET": "x64-mingw-static",
                "VCPKG_HOST_TRIPLET": "x64-mingw-static",
                "CMAKE_PREFIX_PATH": "$env{MSYS2_ROOT}/mingw64",
                "CMAKE_MAKE_PROGRAM": "$env{MSYS2_ROOT}/mingw64/bin/mingw32-make.exe"
            }
        },
        {
            "name": "Darwin",
            "inherits": [
                "base"
            ],
            "hidden": true,
            "displayName": "MacOS (hidden)",
