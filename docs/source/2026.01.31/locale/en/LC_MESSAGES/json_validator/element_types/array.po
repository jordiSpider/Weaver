#: ../json_validator/element_types/array.rst:49
msgid "The '**maxItems**' must be a positive integer."
msgstr ""

#: ../json_validator/element_types/array.rst:51
msgid "Specifies the maximum number of items in the array."
msgstr ""

#: ../json_validator/element_types/array.rst:55
msgid "itemsOrder"
msgstr ""

#: ../json_validator/element_types/array.rst:59
